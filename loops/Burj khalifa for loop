#include <iostream>
using namespace std;
int main(){
    
       for( int row = 1; row <= 10; row++){
    
        
         for( int column = 1; column <= row; column++){
          cout<<"skibidi ";
        } 
        cout<< endl;
        
       }
             for(int row = 10; row >= 1; row--){
    
        
                 for( int column = 1; column <= row; column++){
                cout<<"toilet ";
            } 
                cout<< endl;
        
      
    }
 
    return 0;
}
